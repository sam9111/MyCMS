<h2 class="text-center">Login!</h2>
<section class="form mx-40 my-10 ">
  <%= form_with url: sessions_path do |form| %>
    <p >Email: </p>
    <%= form.email_field :email,class:"form-input my-4"%>
    <p >Password: </p>
    <%= form.password_field :password,class:"form-input my-4"%>
    <div><%= form.submit "Submit" ,class:"form-btn my-5"%></div>
  <% end %>
</section>
